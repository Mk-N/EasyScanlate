QMessageBox {
    background-color: #1E1E1E;
    color: #FFFFFF;
    font-size: 16px;
}
QLabel {
    color: #FFFFFF;
}
QCheckBox {
    color: #FFFFFF;
    spacing: 8px;
}
QCheckBox::indicator {
    width: 18px;
    height: 18px;
}
QPushButton {
    background-color: #3A3A3A;
    color: #FFFFFF;
    min-width: 80px;
    padding: 8px;
    border-radius: 4px;
}
QPushButton:hover {
    background-color: #4A4A4A;
}